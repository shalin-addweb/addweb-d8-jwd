{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/admin_toolbar": "^1.24",
        "drupal/advanced_help": "^1.0@alpha",
        "drupal/backup_migrate": "^4.0",
        "drupal/block_access": "^1.0@alpha",
        "drupal/bootstrap": "^3.11",
        "drupal/commerce": "^2.7",
        "drupal/commerce_shipping": "^2.0@beta",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "^1.0.2",
        "drupal/content_access": "^1.0@alpha",
        "drupal/core": "~8.5.3",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.1",
        "drupal/eu_cookie_compliance": "^1.1",
        "drupal/fences": "^2.0@alpha",
        "drupal/field_collection": "^1.0@alpha",
        "drupal/field_group": "^1.0",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/filefield_sources": "1.x-dev",
        "drupal/fontyourface": "^3.2",
        "drupal/google_analytics": "^2.2",
        "drupal/hierarchical_term_formatter": "^1.0@beta",
        "drupal/honeypot": "^1.27",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/libraries": "^3.0@alpha",
        "drupal/livereload": "1.x-dev",
        "drupal/markdown": "^1.2",
        "drupal/menu_block": "^1.5",
        "drupal/menu_token": "^1.0@alpha",
        "drupal/metatag": "^1.5",
        "drupal/module_filter": "^3.1",
        "drupal/node_export": "1.x-dev",
        "drupal/panels": "^4.3",
        "drupal/pathauto": "^1.2",
        "drupal/redirect": "^1.2",
        "drupal/search_config": "1.x-dev",
        "drupal/smtp": "^1.0@beta",
        "drupal/term_reference_tree": "^1.0@alpha",
        "drupal/token": "^1.3",
        "drupal/token_filter": "^1.0",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_date_format_sql": "3.x-dev",
        "drupal/webform": "^5.0@RC",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
